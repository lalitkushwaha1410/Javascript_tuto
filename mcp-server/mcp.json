{
	"servers": {
		"playwright": {
			"command": "npx",
			"args": [
				"-y",
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		"filesystem": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-filesystem",
				"C:/Nagarro_data/MCP-Server/MCP-output"
			],
			"type": "stdio"
		},
		"excel": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"--yes",
				"@negokaz/excel-mcp-server"
			],
			"env": {
				"EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
			},
			"type": "stdio"
		},
		"github": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-github"
			],
			"type": "stdio"
		},
		"atlassian": {
			"type": "http",
			"url": "https://mcp.atlassian.com/v1/sse",
			"gallery": true
		}
	},
	"inputs": []
}